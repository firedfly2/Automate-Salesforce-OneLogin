{
    "name": "Auto login to SS with OL",
    "version": "0.1.0",
    "manifest_version": 3,
    "description": "OneLogin連携したSalesforceに自動ログインする",
    "content_scripts": [{
      "matches": ["https://*.my.salesforce.com/*"],
      "js": [
        "content_script.js"
      ]
    }],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png"
    }
}
